{
  "name": "themes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "lint": "stylelint \"**/*.scss\"",
    "style": "sass $THEME/scss/style.scss $THEME/css/style.css --no-source-map",
    "watch": "sass $THEME/scss/style.scss $THEME/css/style.css --no-source-map -w",
    "style-min": "sass $THEME/scss/style.scss $THEME/css/style.min.css --no-source-map --style compressed",
    "style-dark": "sass $THEME/scss/style-dark.scss $THEME/css/style-dark.css --no-source-map",
    "watch-dark": "sass $THEME/scss/style-dark.scss $THEME/css/style-dark.css --no-source-map -w",
    "style-min-dark": "sass $THEME/scss/style-dark.scss $THEME/css/style-dark.min.css --no-source-map --style compressed",
    "watch-all": "sass -w $THEME/scss/style.scss:$THEME/css/style.css $THEME/scss/style-dark.scss:$THEME/css/style-dark.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.69.3",
    "stylelint": "^15.10.3",
    "stylelint-config-sass-guidelines": "^10.0.0"
  }
}